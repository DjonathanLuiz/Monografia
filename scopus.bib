@ARTICLE{Vandenbroecke2008121,
author={Van den broecke, F. and Van Landeghem, H. and Aghezzaf, E.-H.},
title={Implementing a near-optimal solution for the multi-stage, multi-product capacitated lot-sizing problem by rolling out a cyclical production plan},
journal={International Journal of Production Economics},
year={2008},
