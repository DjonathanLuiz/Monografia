@ARTICLE{Safari2018531,
author={Safari, G. and Hafezalkotob, A. and Khalilzadeh, M.},
title={A Nash bargaining model for flow shop scheduling problem under uncertainty: a case study from tire manufacturing in Iran},
journal={International Journal of Advanced Manufacturing Technology},
year={2018},
