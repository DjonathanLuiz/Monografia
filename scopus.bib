@ARTICLE{Wang20142055,
author={Wang, L.-C. and Chen, Y.-Y. and Chen, T.-L. and Cheng, C.-Y. and Chang, C.-W.},
title={A hybrid flowshop scheduling model considering dedicated machines and lot-splitting for the solar cell industry},
journal={International Journal of Systems Science},
