  doi                     = {10.1016/j.cor.2010.05.002},
  keywords                = {Computational efficiency; Efficiency; Feedback control; Industrial research; Machine shop practice; Petri nets; Production control; Scheduling, Closed-loop feedback control; Flow-shop scheduling; Industrial implementation; Manufacturing paradigm; One-of-a-kind production; Production efficiency; Production Scheduling; Simulation, Manufacture},
