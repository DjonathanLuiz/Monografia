  author                  = {Wang, B.-L. and Wang, H.-F. and Li, T.-K.},
  journal                 = {Kongzhi yu Juece/Control and Decision},
  title                   = {Scheduling algorithm for permutation flowshop with limited waiting times and rejection [工件可拒绝的有限等待置换流水车间调度算法]},
