@Article{Zandieh2017,
  author                  = {Zandieh, M. and Sajadi, S.M. and Behnoud, R.},
  journal                 = {International Journal of Systems Assurance Engineering and Management},
  title                   = {Integrated production scheduling and maintenance planning in a hybrid flow shop system: a multi-objective approach},
  year                    = {2017},
