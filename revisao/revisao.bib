@CONFERENCE{Sommerhalter2011,
author={Sommerhalter, M. and Santos, D.L.},
title={A scheduling tool for processing large-area flexible electronics},
journal={61st Annual IIE Conference and Expo Proceedings},
year={2011},
note={cited By 0; Conference of 61st Annual Conference and Expo of the Institute of Industrial Engineers ; Conference Date: 21 May 2011 Through 25 May 2011;  Conference Code:104966},
url={https://www.scopus.com/inward/record.uri?eid=2-s2.0-84900297121&partnerID=40&md5=50235564b8817b7ceb0b8b8e539e0eb8},
affiliation={Systems Science and Industrial Engineering Department, Binghamton University - State University of New York, 4400 Vestal Parkway East, Binghamton, NY 13902-6000, United States},
abstract={The scheduling of state-of-the-art equipment for manufacturing microelectronic circuitry on flexible substrates is considered. Three major processing steps are the following: vacuum deposition, photolithography, and photoresist development. Customers of this facility have different needs and thus have different processing time requirements, different due-dates, etc. The n-job, three machine flowshop problem, while relatively trivial for some performance measures, can be NP-complete for others (e.g., tardiness-based criteria). A scheduling tool to be used in conjunction with the Lekin ® software created at NYU is being developed. Data are entered for the scheduling instance, namely, the number of jobs, processing times, due-dates, weights, if applicable, and the performance measure of interest. Lekin-based files will then be created from the data and used as inputs into the Lekin software. Intelligence, in the form of heuristics, is being embedded into the system. For example, if the performance measure is makespan and it is determined that the maximum time on the photoresist tool is no greater than the minimum time on the other tools, then a special application of Johnson's Algorithm will identify the optimal permutation schedule to process the jobs. The primary output of the system is the recommended schedule of the jobs, replete with Gantt chart.},
author_keywords={Flow shops;  Interactive tool;  Lekin®;  Production scheduling;  R2R electronics manufacturing},
