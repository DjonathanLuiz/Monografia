@Article{Toncovich2019,
  author                  = {Toncovich, A.A. and Rossit, D.A. and Frutos, M. and Rossit, D.G.},
  journal                 = {International Journal of Industrial Engineering Computations},
  title                   = {Solving a multi-objective manufacturing cell scheduling problem with the consideration of warehouses using a simulated annealing based procedure},
