@Article{Zheng2018,
  author                  = {Zheng, P. and Li, S.},
  journal                 = {Academic Journal of Manufacturing Engineering},
  title                   = {Flow shop scheduling optimization and simulation analysis based on Q-learn algorithm and process value evaluation algorithm},
  year                    = {2018},
