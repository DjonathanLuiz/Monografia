@Article{Liu2014,
  author                  = {Liu, C.-H. and Huang, D.-H.},
  journal                 = {International Journal of Production Research},
  title                   = {Reduction of power consumption and carbon footprints by applying multi-objective optimisation via genetic algorithms},
  year                    = {2014},
