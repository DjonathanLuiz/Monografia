@InProceedings{Richter2017,
  author     = {Richter, M. and Winkler, H. and Fertsch, M. and Stachowiak, A. and Mrugalska, B. and Oleskow-Szlapka, J. and Hadas, L. and Cyplik, P. and Golinska-Dawson, P. and Articles, Andre Abrasive and Fana and Lighting, Philips and Poznan, Volkswagen},
  title      = {Flow shop scheduling optimization in the chipboard industry: A simulation-based analysis using priority dispatching rules},
  year       = {2017},
  address    = {Brandenburg University of Technology Cottbus-Senftenberg, Siemens-Halske-Ring 6, Cottbus, Germany},
  pages      = {296--301},
  publisher  = {DEStech Publications},
  abstract   = {This paper gives insights on the usability of single priority and multi-criteria dispatching rules for production scheduling based on the results for a German chipboard manufacturing plant. A discrete-event simulation of the manufacturing process has been modeled to compare different priority rules with an expert-based sequence for a real set of jobs. The simulation results show that using one single or multi-criteria priority dispatching rule on the total production system basically leads to a poor performance, whereas using different rules for each machine will enhance the overall performance. Higher performance can be achieved by the expert-based scheduling and individual multi-criteria dispatching rules. Hence, a dynamic modification of the production schedule by switching the dispatching rules according to the current performance is proposed, considering unforeseen disturbances. © 2017 DEStech Publications. All rights reserved.},
